{
  "display": "Node 18",
  "_version": "18.2.0",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "esnext"],
    "module": "esnext",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "noImplicitAny": false
  }
}
